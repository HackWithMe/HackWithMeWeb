
peoplefile = open("DemoPeopleInterests.txt", 'r')
personfile = open("DemoPersonInterests.txt", 'r')
    



interests = []


s = peoplefile.read()
s = s.split(',')

for i in range(len(s)):
    if s[i].strip() == '': continue
    interests.append(s[i])

for i in range(len(s)):
    if s[i].strip() == '': continue
    interests.append(s[i])


peoplefile.close()
personfile.close()